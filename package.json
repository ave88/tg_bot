{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "description": "Telegram bot",
  "author": "Konovalov Artem",
  "license": "",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "prestart": "rimraf dist && tsc",
    "start:dev": "NODE_ENV=dev nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "NODE_ENV=prod NODE_PATH=./dist node dist/src/main.js",
    "barrels:generate": "barrelsby -c barrelsby.json -q && yarn tslint:fix"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.3",
    "@nestjs/core": "^6.10.3",
    "@nestjs/mongoose": "^6.1.2",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.16",
    "nestjs-config": "^1.3.21",
    "node-telegram-bot-api": "^0.40.0",
    "puppeteer": "^2.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^3.0.0",
    "rxjs": "^6.2.2",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.121",
    "@types/mongoose": "^5.3.20",
    "@types/node": "^12.12.14",
    "@types/node-telegram-bot-api": "^0.40.0",
    "@types/puppeteer": "^2.0.0",
    "barrelsby": "^2.0.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.14.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.5.0",
    "tslib": "^1.9.3",
    "tslint": "^5.20.1",
    "tslint-config-security": "^1.15.0",
    "tslint-microsoft-contrib": "^6.0.0"
  }
}
